   "source": [
    "number = input()\n",
    "length = len(number) \n",
    "sum = 0\n",
    "\n",
    "for digit in number:\n",
    "    sum = sum + (int(digit) ** length) \n",
    "   \n",
    "if sum==int(number):\n",
    "    print(\"Armstrong Number\")\n",
    "else:\n",
    "    print(\"Not an Armstrong Number\")"
   ]
  }
