    "n=input()\n",
    "l=len(n)\n",
    "\n",
    "n1=n[0]\n",
    "n2=n[1]\n",
    "n3=n[2]\n",
    "n4=n[3]\n",
    "\n",
    "sum=(int(n1)**l)+(int(n2)**l)+(int(n3)**l)+(int(n4)**l)\n",
    "if (sum)==int(n):\n",
    "    print(\"Armstrong Number\")\n",
    "else:\n",
    "    print(\"Not an Armstrong Number\")"
   ]
